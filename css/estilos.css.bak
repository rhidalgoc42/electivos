.intro2{font-size:20px;border:1px solid #527e7f;padding:0px;margin:0px 0 0px 0;border-radius:8px;}
.contenido2{width:860px;background-color:#527e7f;border-radius:8px;}
@media screen and (max-width:479px) {
#header{font-size:14px;line-height:22px;}
}

