/* Chrome, Safari, Opera */
@-webkit-keyframes moverlogo {
    0%   {left:200px;top: 80px;}
    25%  {left:730px;top: 80px;}
    50%  {left:730px;top: 470px;}
    75%  {left:200px;top: 470px;}
    100% {left:200px;top: 80px;}
}

/* Standard syntax */
@keyframes moverlogo {
    0%   {left:200px;top: 80px;}
    25%  {left:730px;top: 80px;}
    50%  {left:730px;top: 470px;}
    75%  {left:200px;top: 470px;}
    100% {left:200px;top: 80px;}
}

